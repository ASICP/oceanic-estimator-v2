  const [currentStep, setCurrentStep] = useState(1);\
  const [wizardData, setWizardData] = useState<WizardData>(\{\});\
  const [isCompleted, setIsCompleted] = useState(false);\
  const handleStepData = (step: number, data: any) => \{\
    setWizardData(prev => (\{\
      ...prev,\
      [`step$\{step\}`]: data\
    \}));\
  \};\
  const handleNext = () => \{\
    if (currentStep < 5) \{\
